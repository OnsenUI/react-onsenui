{
  "name": "react-onsenui",
  "version": "0.2.7",
  "description": "Onsen UI - React Components for Hybrid Cordova/PhoneGap Apps with Material Design and iOS UI components",
  "main": "dist/react-onsenui.js",
  "scripts": {
    "build": "npm run lint && rollup -c",
    "prepublish": "npm run build",
    "lint": "eslint --ext .js,.jsx src",
    "lint-fix": "eslint --ext .js,.jsx src --fix",
    "watch-onsenui": "nodemon -x \"npm run build\" --watch OnsenUI/build/js/onsenui.js",
    "watch": "nodemon -x \"npm run build\" --watch src -e jsx",
    "dev": "npm run watch & npm run watch-onsenui & watchify -vd -t [ babelify --presets es2015,react --ignore OnsenUI/build/js/onsenui.js ] -t [ aliasify --aliases [ --react-onsenui=./dist/react-onsenui.js ] ] -e demo/index.js -o demo/bundle.js & http-server . -c 1",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OnsenUI/react-onsenui.git"
  },
  "keywords": [
    "react",
    "react-component",
    "phonegap",
    "cordova",
    "hybrid",
    "app",
    "mobile",
    "ui",
    "onsen",
    "onsenui"
  ],
  "author": "Onsen UI Team <team@monaca.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OnsenUI/react-onsenui/issues"
  },
  "homepage": "https://onsen.io/v2/",
  "devDependencies": {
    "aliasify": "^1.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "eslint": "^2.5.3",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "nodemon": "^1.9.1",
    "onsenui": "^2.0.0-rc.4",
    "react": "^15.0.2",
    "react-docgen": "2.8.2",
    "react-dom": "^15.0.2",
    "rollup": "^0.26.2",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-replace": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "peerDependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "onsenui": "2.0.0-rc.4"
  },
  "dependencies": {
    "babel-loader": "^6.2.4",
    "chai": "^3.5.0",
    "expect": "^1.20.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "react-dom": "^15.0.2",
    "webpack": "^1.13.0"
  }
}
